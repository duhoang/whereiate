$text-color: #3b3b3b;
$gray:#e0e0e0;
$main-width:960px;
$black:#000;
$text-black:#3b3b3b;
$white:#fff;
$blue:#00b0e3;
$dark-gray:#747474;

$s:#ffffff;
$e:#e0e0e0;

@mixin gradient-hori($c-start, $start, $c-end, $end) {
	
	background: -moz-linear-gradient(left,  $c-start $start, $c-end $end); /* FF3.6+ */
	background: -webkit-gradient(linear, left top, right top, color-stop($start,$c-start), color-stop($end,$c-end)); /* Chrome,Safari4+ */
	background: -webkit-linear-gradient(left,  $c-start $start,$c-end $end); /* Chrome10+,Safari5.1+ */
	background: -o-linear-gradient(left,  $c-start $start,$c-end $end); /* Opera 11.10+ */
	background: -ms-linear-gradient(left,  $c-start $start,$c-end $end); /* IE10+ */
	background: linear-gradient(to right,  $c-start $start,$c-end $end); /* W3C */
	-ms-filter: "progid:DXImageTransform.Microsoft.gradient(GradientType=1, startColorstr='#{ie-hex-str($c-start)}', endColorstr='#{ie-hex-str($c-end)}');";
	  filter: progid:DXImageTransform.Microsoft.gradient(GradientType=1, startColorstr='#{ie-hex-str($c-start)}', endColorstr='#{ie-hex-str($c-end)}');;
	  zoom: 1;

}

* {
	box-sizing:border-box;
	-moz-box-sizing:border-box;
	-webkit-box-sizing:border-box;
	-ms-box-sizing:border-box;
}

html, body {
	width:100%;
	height:100%;
	font-size:12px;
	color:$text-color;
font-family: 'PT Sans Caption', sans-serif;
	line-height:1.3;
	min-width:320px;
	-webkit-text-size-adjust: none;
    -ms-text-size-adjust: none;
	-moz-text-size-adjust: none;
    text-size-adjust: none;
}

.clear {
	clear:both;
}

a, a:hover, a:visited {
	color:inherit;
	text-decoration:none;
}

.header {
	position:fixed;
	z-index:10;
	width:100%;
	margin:0 auto;
	
	@media only screen 
	and (max-width : 640px) {
		position:relative;
	}
}

.header-inner {
	max-width:1280px;
	min-width:320px;
	margin:0 auto;
	
}

.header-wrap {
	margin:0 4px;
	padding:0 8px;
	background:rgba(white, .84);
	
	@media only screen 
	and (max-width : 640px) {
		padding:0;
	}
}

.head-left {
	float:left;
	color:#b0b0b0;
	
	white-space:nowrap;
	font-size:12px;
	height:36px;
	padding-left:8px;
	strong {
		font-size:24px;
		padding-right:0px;
		
		
	}
	
	@media only screen 
		and (max-width : 640px) {
		width:100%;
		
		strong {
			font-size:20px;
			display:block;
			padding-top:4px;
		}
		
		
	}
	
	
	a {
		padding:0 8px;
		
		&:hover {
			color:$blue;
		}
		
		
		
		@media only screen 
		and (max-width : 640px) {
			color:$dark-gray;
			font-size:10px;
			
		}
		
		&:first-child {
			padding-left:0;
			float:left;
			
			padding-top:4px;
			@media only screen 
			and (max-width : 640px) {
				color:$text-black;
			}
		}
	}
	
}

.head-right {
	color:#b0b0b0;
	float:right;
	white-space:nowrap;
	font-size:14px;
	height:36px;
	line-height:36px;
	a {
		padding:8px;
		font-weight:bold;
		
		&:hover, &.active {
			color:$blue;
		}
	}
	
	@media only screen 
		and (max-width : 640px) {
			text-align:center;
			width:100%;
			border-top:1px solid $gray;
			padding-right:0;
			font-size:12px;
		}
}

.main {
	max-width:1280px;
	min-width:320px;
	height:100%;
	margin:0 auto;
	
	@media only screen 
		and (min-width :640px) 
		and (max-width : 768px) {
		padding-top:36px;
	}
	
}

.page {
	margin:0 4px;
	height:100%;
	position:relative;
	overflow:hidden;
	
	@media only screen 
	and (max-width : 768px) {
		padding:0;
		height:auto;
	}
}

.page-bg {
	width:100%;
	max-width:960px;
	height:100%;
	max-height:640px;
	position:absolute;
	right:0;
	top:36px;
	z-index:1;
	
		
	@media only screen 
	and (max-width : 768px) {
		position:relative;
		height:480px;
		top:0;
	}
	
	@media only screen 
	and (max-width : 640px) {
		position:relative;
		height:320px;
		top:0;
	}
	
	.about &{
		background:url(../img/about.jpg) top center;
		background-size:cover;
	}
	
	.about0 &{
		background:url(../img/about0.jpg) top center;
		background-size:cover;
	}
	
	.about1 &{
		background:url(../img/about1.jpg) top center;
		background-size:cover;
	}
	
	.about2 &{
		background:url(../img/about2.jpg) top center;
		background-size:cover;
	}
	
	.screeningroom &{
		background:url(../img/screeningroom.jpg) top center;
		background-size:cover;
	}
	
	.audiopilot &{
		background:url(../img/audiopilot.jpg) top center;
		background-size:cover;
	}
	
	.shooter &{
		background:url(../img/3dshooter.jpg) top center;
		background-size:cover;
	}
	
	.spoton &{
		background:url(../img/spoton.jpg) top center;
		background-size:cover;
	}
	
	.amex &{
		background:url(../img/amex.jpg) top center;
		background-size:cover;
	}
	
	.hashable &{
		background:url(../img/hashable.jpg) top center;
		background-size:cover;
	}
	
	.patron &{
		background:url(../img/patron.jpg) top center;
		background-size:cover;
	}
	
	.caloriecount &{
		background:url(../img/cc0.jpg) top center;
		background-size:cover;
	}
	
	.cc1 &{
		background:url(../img/cc1.jpg) top center;
		background-size:cover;
	}
	
	.cc2 &{
		background:url(../img/cc2.jpg) top center;
		background-size:cover;
	}
	
	
	iframe {
		width:100%;
		height:100%;
		border:1px solid black;
	}
}

.page-content {
	width:320px;
	padding:88px 8px 40px 16px;
	height:100%;
	line-height:1.5;
	z-index:2;
	position:relative;
	pointer-events:none;
	text-shadow:0 0 8px white;
	
	p {
		margin-bottom:8px;
	}		
	
	@media only screen 
	and (max-width : 768px) {
		width:100%;
		height:auto;
		padding-top:8px;
		padding-right:8px;
		pointer-events:auto;
		
	}
	
	background: -moz-linear-gradient(left,  rgba(255,255,255,1) 0%, rgba(255,255,255,1) 30%, rgba(255,255,255,0.8) 90%, rgba(255,255,255,0) 100%); /* FF3.6+ */

background: -webkit-gradient(linear, left top, right top, color-stop(0%,rgba(255,255,255,1)), color-stop(30%,rgba(255,255,255,1)), color-stop(90%,rgba(255,255,255,0.8)), color-stop(100%,rgba(255,255,255,0))); /* Chrome,Safari4+ */

background: -webkit-linear-gradient(left,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 30%,rgba(255,255,255,0.8) 90%,rgba(255,255,255,0) 100%); /* Chrome10+,Safari5.1+ */

background: -o-linear-gradient(left,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 30%,rgba(255,255,255,0.8) 90%,rgba(255,255,255,0) 100%); /* Opera 11.10+ */

background: -ms-linear-gradient(left,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 30%,rgba(255,255,255,0.8) 90%,rgba(255,255,255,0) 100%); /* IE10+ */

background: linear-gradient(to right,  rgba(255,255,255,1) 0%,rgba(255,255,255,1) 30%,rgba(255,255,255,0.8) 90%,rgba(255,255,255,0) 100%); /* W3C */

filter: progid:DXImageTransform.Microsoft.gradient( startColorstr='#ffffff', endColorstr='#00ffffff',GradientType=1 ); /* IE6-9 */


}

.page-c-inner {
	
	a {
		display:none;
	}
	
	@media only screen 
	and (max-width : 768px) {
		padding:0 8px;
		
		a {
			font-size:16px;
			margin-top:28px;
			padding-top:8px;
			color:$blue;
			display:block;
			border-top:1px solid $gray;
			font-weight:bold;
			text-align:right;
			
		}
	}
	
}

.logo {
	margin-bottom:12px;
	
	
	.about0 &{
		width:120px;
		height:20px;
		background:url(../img/logos.png);
	}
	
	.hashable &{
		width:113px;
		height:20px;
		background:url(../img/logo-hashable.png);
	}	
	
	.amex &{
		width:60px;
		height:58px;
		background:url(../img/logo-amex.jpg);
	}
	
	.patron &{
		width:90px;
		height:60px;
		background:url(../img/patron-logo.png) ;
	}
	
	.caloriecount &{
		width:165px;
		height:30px;
		background:url(../img/logo-cc.png) ;
		background-size:165px 30px;
	}
	
	
	
	@media only screen 
		and (max-width : 640px) {
		margin-top:8px;
		
		
		
	}
	
}

.page-heading {

	font-weight:bold;
	font-size:14px;
	margin-bottom:8px;
}

.connect-item {
	display:block;
	float:left;
	width:24px;
	height:24px;
	background:url(../img/connect-sprites.png) #c0c0c0 no-repeat;
	margin:6px 0 0 16px;
	border-radius:100%;
	
	&:hover {
		background-color:$blue;
	}
	
	&.email {
		background-position:0 center;
	}
	
	&.twitter {
		background-position:-24px center;
	}
	
	&.linkedin {
		background-position:-48px center;
	}
	
	@media only screen 
		and (max-width : 640px) {
		
		}	
}

.connect-wrap {
	float:right;
	
	@media only screen 
		and (max-width : 640px) {
		float:right;
		padding-right:8px;
	}
}	
